@model NguyenPhucThienTinh_DH52201572.Models.Nhasanxuat
<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<!-- Latest compiled and minified CSS -->
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css"
		  rel="stylesheet">
	<!-- Latest compiled JavaScript -->
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js">
	</script>
</head>
<body>
	<div class="container">
		<form asp-action="them" method="post">
			<h2>Thêm nhà sản xuất</h2>
			<div class="form-group">
				<div asp-validation-summary="ModelOnly" class="text-danger">
					<span>Các lỗi cần khắc phục:</span>
				</div>
			</div>
			<div class="form-group">
				<label asp-for="Mansx" class="form-label"></label>@*<br />*@
				<input asp-for="Mansx" class="form-control" />@*<br /><br />*@
				<span asp-validation-for="Mansx" class="text-danger"></span>
			</div>
			<div class="form-group">
				<label asp-for="Tennsx" class="form-label"></label>
				<input asp-for="Tennsx" class="form-control" />
				<span asp-validation-for="Tennsx" class="text-danger"></span>
			</div>
			<div class="form-group">
				<label asp-for="Diachi" class="form-label"></label>
				<input asp-for="Diachi" class="form-control" />
				<span asp-validation-for="Diachi" class="text-danger"></span>
			</div>
			<div class="form-group mt-3">
				<input type="submit" class="btn btn-primary" value="Save" /><br /><br />
			</div>
			<div>
				<a asp-action="Index">Quay lại</a>
			</div>
		</form>
	</div>
</body>
</html>

@model NguyenPhucThienTinh_DH52201572.Models.Nhasanxuat

<div class="container">
	<h2>Sửa nhà sản xuất</h2>
	<form asp-action="Sua" method="post">
		<div class="form-group">
			<label asp-for="Mansx" class="form-label"></label>
			<input asp-for="Mansx" class="form-control" readonly />
		</div>
		<div class="form-group">
			<label asp-for="Tennsx" class="form-label"></label>
			<input asp-for="Tennsx" class="form-control" />
			<span asp-validation-for="Tennsx" class="text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="Diachi" class="form-label"></label>
			<input asp-for="Diachi" class="form-control" />
			<span asp-validation-for="Diachi" class="text-danger"></span>
		</div>
		<div class="form-group mt-3">
			<button type="submit" class="btn btn-primary">Lưu</button>
			<a asp-action="Index" class="btn btn-secondary">Quay lại</a>
		</div>
	</form>
</div>
